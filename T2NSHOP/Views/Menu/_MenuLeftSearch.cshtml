
@model IEnumerable<T2NSHOP.Models.EF.ProductCategory>

@if (Model != null && Model.Any())
{
    <ul class="sidebar_categories">
        @foreach (var item in Model)
        {
            if (ViewBag.CateId == item.Id)
            {
                <li class="active"><a href="/tim-kiem/@item.Alias-@(item.Id)"><span><i class="fa fa-angle-double-right" aria-hidden="true"></i></span>@item.Titel</a></li>
            }
            else
            {
                <li><a href="/tim-kiem/@item.Alias-@(item.Id)">@item.Titel</a></li>
            }
        }
    </ul>
}

