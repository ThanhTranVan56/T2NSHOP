@model IEnumerable<T2NSHOP.Models.EF.Categogy>

@if (Model != null && Model.Any())
{
    <ul class="navbar_menu">
        @foreach (var item in Model)
        {
            if (item.Alias.Equals("trang-chu"))
            {
                item.Alias = "";
            }
            <li><a href="/@item.Alias">@item.Titel</a></li>
        }
    </ul>
}

